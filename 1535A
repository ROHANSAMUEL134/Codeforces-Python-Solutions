for _ in range(int(input())):
    a,b,c,d=map(int,input().split())
    if a>b:
        max1=a
        min1=b
    else:
        max1=b
        min1=a
    if c>d:
        max2=c
        min2=d
    else:
        max2=d
        min2=c
    if min1>max2 or min2>max1:
        print("NO")
    else:
        print("YES")
