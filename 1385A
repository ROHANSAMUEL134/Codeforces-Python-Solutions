for _ in range(int(input())):
    a = list(map(int, input().split()))
    a.sort()
    if (a[0] != a[1] and a[1] != a[2]) or (a[0] == a[1] and a[1] != a[-1]):
        print("NO")
    else:
        print("YES")
        if a[0] == a[-1]: print(a[0], a[0], a[0])
        else: print(a[0], a[0], a[-1])
    
